{
  "name": "objective-note",
  "private": true,
  "version": "0.0.0",
  "main": "electron.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
